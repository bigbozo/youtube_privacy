{
  "name": "ichhabrecht/youtube-privacy",
  "type": "typo3-cms-extension",
  "description": "Render YouTube videos with nocookie domain",
  "keywords": [
    "TYPO3",
    "youtube",
    "cookie"
  ],
  "homepage": "https://github.com/IchHabRecht/TYPO3-youtube_privacy",
  "authors": [
    {
      "name": "Nicole Cordes",
      "email": "typo3@cordes.co",
      "role": "Developer"
    }
  ],
  "license": "GPL-2.0+",
  "require": {
    "typo3/cms-core": ">=7.6,<9"
  },
  "suggest": {
    "friendsofphp/php-cs-fixer": "Tool to automatically fix PHP coding standards issues"
  },
  "replace": {
    "youtube_privacy": "self.version",
    "typo3-ter/youtube_privacy": "self.version"
  }
}
